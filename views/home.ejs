<%- include("partials/header.ejs") %>

  <!-- Overlay When Open Navigation Bar -->
  <div id="bodyOverlay"> </div>

  <!-- Menu Button for Mobile -->
  <div class="button-menu" id="menuToggle">
    <input type="checkbox" />
    <span></span>
    <span></span>
    <span></span>
  </div>

  <!-- Title -->
  <div id="sidebarMenu" class="px-4">
    <div class="today mb-2 ">
      <h1 class="mb-0"">
        <%=homeTitle%>
      </h1>
      <p class="mb-1">
        <%=date%>
          </p>
    </div>
  </div>

  <!-- Content -->
  <div class="page-content p-4 " id="content">
    <div class="content">
      <%newTaskItem.forEach((item)=>{%>
        <form action = "/delete" method="post">
          <input type="hidden" value="<%= homeTitle%>" name="lists">
          <div class="items">
            <input type="checkbox">
            <p class="mb-0">
              <%=item.name%>
            </p>
          </div>
          <%});%>
        </form>
    </div>
  </div>
  <%- include("task.ejs") %>


    <%- include("partials/footer.ejs") %>