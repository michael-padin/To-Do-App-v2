<%- include("partials/header.ejs") %>
  <!-- Overlay When Open Navigation Bar -->
  <div id="bodyOverlay"> </div>

  <!-- Menu Button for Mobile -->
  <div class="button-menu" id="menuToggle">
    <input type="checkbox" />
    <span></span>
    <span></span>
    <span></span>
  </div>

  <!-- Header  -->
  <div id="sidebarMenu" class="px-4">
    <div class="today mt-2 mb-2 ">
      <h1 class="listTitle text-white" name="listTitle">
        <%=homeTitle%>
      </h1>
    </div>
  </div>


 <!-- Content -->
    <div class="page-content p-4 " id="content">
      <div class="content">
        <%newTaskItem.forEach((item)=>{%>
          <div class="items">
            <i class="delete far fa-trash-alt"></i>
            <input type="checkbox">
            <p class="mb-0" contenteditable="true">
              <%=item.name%>
            </p>
          </div>
          <%});%>
      </div>
    </div>

  <!-- Add NeW Task -->
  <%- include("task.ejs") %>
<%- include("partials/footer.ejs") %>